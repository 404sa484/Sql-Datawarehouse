-- Checking when cid isnt match with cst_key from crm_cust_info table
SELECT *
from bronze.erp_cust_az12
where cid not like 'NAS%'

select*, 
substring(cid,4,len(cid)) as cid from bronze.erp_cust_Az12 

---second column bdate if any perseon greater than today date
select distinct
bdate
from bronze.erp_cust_Az12
where bdate<getdate() or bdate<='1926-02-06'

---check on gender column

select distinct
gen
from bronze.erp_cust_Az12

select
case
when upper(trim(gen))='F' or upper(trim(gen))='Female' then 'Female'
when upper(trim(gen))='M' or upper(trim(gen))='Male' then 'Male'
else 'n/a'
end as gen
from bronze.erp_cust_Az12

