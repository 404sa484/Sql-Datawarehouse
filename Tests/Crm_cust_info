---write a query to check if nay duplicates are there in 

select cst_id,count(*) from
silver.crm_cust_info
group by cst_id
having count(*)>1;

----check for unwated spaces in firstname

select cst_firstname from silver.crm_cust_info
where cst_firstname!=trim(cst_firstname);

----check for unwated spaces in lastname

select cst_lastname from silver.crm_cust_info
where  cst_lastname!=trim( cst_lastname);

select cst_lastname from silver.crm_cust_info
where cst_lastname is null;

select cst_firstname from silver.crm_cust_info
where cst_firstname is null;

-----check values in gender and Martia_status columns 

select distinct cst_gndr from silver.crm_cust_info

select distinct cst_marital_status from silver.crm_cust_info


